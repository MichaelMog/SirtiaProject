<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ScreenNavigationController">
   <children>
      <Button fx:id="viewReportsButton" layoutX="429.0" layoutY="258.0" mnemonicParsing="false" onAction="#viewReportsScreen" prefHeight="38.0" prefWidth="144.0" text="View Reports" />
      <Button fx:id="updateContentButton" layoutX="429.0" layoutY="100.0" mnemonicParsing="false" onAction="#updateContentScreen" prefHeight="38.0" prefWidth="144.0" text="Update Content" />
      <Button fx:id="exploreMoviesButton" layoutX="429.0" layoutY="25.0" mnemonicParsing="false" onAction="#exploreMoviesScreen" prefHeight="38.0" prefWidth="144.0" text="Explore Movies" />
      <Label alignment="CENTER" layoutX="42.0" layoutY="25.0" prefHeight="38.0" prefWidth="258.0" text="Screen Navigation Screen!" textAlignment="CENTER" textFill="#0b0b11" textOverrun="CENTER_ELLIPSIS" underline="true">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button fx:id="purchaseSubscriptionButton" layoutX="429.0" layoutY="181.0" mnemonicParsing="false" onAction="#purchaseSubscriptionScreen" prefHeight="38.0" prefWidth="144.0" text="Purchase Subscription" />
      <Label layoutX="14.0" layoutY="331.0" text="To check entries left in subscription:" />
      <TextField fx:id="subTF" layoutX="216.0" layoutY="327.0" prefHeight="25.0" prefWidth="190.0" promptText="enter the subscription name!" />
      <Button fx:id="subButton" layoutX="180.0" layoutY="361.0" mnemonicParsing="false" onAction="#checkSub" text="Check" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.HandleComplaintsController">
   <top>
        <HBox prefHeight="51.0" prefWidth="600.0" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#BackTo" text="Back">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Button>
                <Button fx:id="ShowComplaints" mnemonicParsing="false" onAction="#ShowAllComplaints" prefHeight="40.0" prefWidth="145.0" text="Show All Complaints">
               <HBox.margin>
                  <Insets top="4.0" />
               </HBox.margin>
            </Button>
            </children>
        </HBox>
   </top>
   <center>
        <HBox prefHeight="331.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <ScrollPane prefHeight="331.0" prefWidth="397.0">
                    <content>
                        <VBox fx:id="complaintsList" />
                    </content>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ScrollPane>
                <VBox fx:id="complaintMore" prefHeight="331.0" prefWidth="223.0" />
            </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </BorderPane.margin>
        </HBox>
   </center>
</BorderPane>
